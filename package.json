{
  "name": "nextjs-advanced-starter",
  "version": "1.0.0",
  "description": "Next.js Advanced Starter with Typescript, MaterialUI, Storybook, Jest, Puppeteer, Redux, ESLint, Prettier and more.",
  "main": "dist/index.js",
  "private": true,
  "scripts": {
    "format:check": "yarn prettier --check .",
    "format:write": "yarn prettier --write .",
    "lint": "eslint --ext js,ts,tsx ./src",
    "dev": "nodemon",
    "build": "rimraf dist && next build && tsc --project ./tsconfig.server.json",
    "start": "cross-env NODE_ENV=production node ./dist",
    "test": "jest --coverage",
    "test:update": "jest -u",
    "test:puppeteer": "node ./test/index.js headless=false",
    "test:puppeteer:headless": "node ./test/index.js headless=true",
    "test:clear-cache": "jest --clearCache",
    "test:watch": "jest --watch",
    "storybook": "start-storybook -p 6006 --no-dll",
    "build-storybook": "build-storybook --no-dll"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/brianpunzalan/nextjs-advanced-starter.git"
  },
  "keywords": [
    "nodejs",
    "reactjs",
    "nextjs",
    "starter"
  ],
  "author": "Brian Punzalan (punzalan25brian@gmail.com)",
  "license": "MIT",
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/styles": "^4.10.0",
    "@reduxjs/toolkit": "^1.4.0",
    "@types/express": "^4.17.9",
    "@types/node": "^14.14.7",
    "@types/puppeteer": "^5.4.0",
    "@types/react": "^16.9.56",
    "@types/react-dom": "^16.9.9",
    "@types/react-redux": "^7.1.11",
    "axios": "^0.21.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "next": "^9.5.2",
    "prop-types": "^15.7.2",
    "puppeteer": "^5.5.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-typescript": "^7.12.1",
    "@storybook/addon-actions": "^6.0.28",
    "@storybook/addon-essentials": "^6.0.28",
    "@storybook/addon-links": "^6.0.28",
    "@storybook/react": "^6.0.28",
    "@types/enzyme": "^3.10.8",
    "@types/jest": "^26.0.15",
    "@types/react-test-renderer": "^16.9.3",
    "@typescript-eslint/eslint-plugin": "^4.7.0",
    "@typescript-eslint/parser": "^4.7.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.1",
    "babel-plugin-styled-components": "^1.11.1",
    "chalk": "^4.1.0",
    "cross-env": "^7.0.2",
    "enzyme": "^3.11.0",
    "eslint": "^7.13.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "jest": "^26.6.3",
    "nodemon": "^2.0.6",
    "prettier": "2.1.2",
    "react-is": "^17.0.1",
    "react-test-renderer": "^17.0.1",
    "rimraf": "^3.0.2",
    "simon": "^0.2.3",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.5"
  }
}
